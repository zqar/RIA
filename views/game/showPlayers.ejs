<% for (var i = 0, j = players.length ; i < j ; i++) {%>
	<div class='<% if(players[i].IsActive()){%>active<%} else {%>inactive<%}%>'>
		<div class="playerName">
			<%= players[i].GetName()%>
		</div>
		<div class="score">
			Score: <%= players[i].GetScore()%>
		</div>
	</div>
<%}%>

